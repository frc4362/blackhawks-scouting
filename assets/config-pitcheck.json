{
  "heading": "2024 Pit Crew Prematch Checklist",
  "logo": "gemslogo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Pit check sign-in",
      "widgets": [
        {
          "name": "Pit Crew Member",
          "type": "radio",
          "options": [
            "Kyle",
            "Chris",
            "Will",
            "Rowan",
            "Other"
          ],
          "default": 4
        }
      ]
    },
    {
      "name": "Pit check",
      "widgets": [
        {
          "name": "Bumper Color",
          "type": "radio",
          "options": [
            "Red",
            "Blue",
            "Black"
          ]
        },
        {
          "name": "Battery Voltage",
          "type": "number",
          "verification": {
            "comparison": "greater",
            "value": 0
          }
        },
        {
          "name": "Battery connected + wires safe? (click)",
          "type": "checkbox"
        },
        {
          "name": "Limelight Connected",
          "type": "checkbox"
        },
        {
          "name": "Motor and Canivore status lights good",
          "type": "checkbox"
        },
        {
          "name": "Absolute encoder status lights good",
          "type": "checkbox"
        },
        {
          "name": "Roborio status lights good",
          "type": "checkbox"
        },
        {
          "name": "Climb prepped?",
          "type": "checkbox"
        },
        {
          "name": "Rollers good",
          "type": "checkbox"
        },
        {
          "name": "Bumper height / wingnut tightness",
          "type": "checkbox"
        }
      ]
    }
  ]
}
