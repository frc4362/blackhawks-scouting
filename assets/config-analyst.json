{
  "heading": "2024 Analyst Form",
  "logo": "gemslogo.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Analyst Name",
          "type": "radio",
          "options": [
            "Grant",
            "Sam",
            "Kaleb",
            "Reese",
            "Other"
          ],
          "default": 4
        }
      ]
    },
    {
      "name": "Match",
      "widgets": [
        {
          "name": "Amp Rating",
          "type": "number",
          "validation": {
            "comparison": "inRange",
            "value": [1, 3]
          }
        },
        {
          "name": "Driver Rating",
          "type": "number",
          "validation": {
            "comparison": "inRange",
            "value": [1, 3]
          }
        },
        {
          "name": "Shooting Ranges: Subwoofer",
          "type": "checkbox"
        },
        {
          "name": "Shooting Ranges: Starting Line",
          "type": "checkbox"
        },
        {
          "name": "Shooting Ranges: Podium",
          "type": "checkbox"
        },
        {
          "name": "Shooting Ranges: Mid-stage",
          "type": "checkbox"
        },
        {
          "name": "Shooting Ranges: Back-stage",
          "type": "checkbox"
        },
        {
          "name": "Spotlighting?",
          "type": "checkbox"
        },
        {
          "name": "Notes",
          "type": "textarea"
        }
      ]
    }
  ]
}
